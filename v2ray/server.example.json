{
  "log": {
    "loglevel": "warning"
  },
  "policy": {
    "levels": {
      "0": {
        "handshake": 10,
        "connIdle": 300,
        "uplinkOnly": 0,
        "downlinkOnly": 0,
        "statsUserUplink": true,
        "statsUserDownlink": true,
        "bufferSize": 2048
      }
    },
    "system": {
      "statsInboundUplink": true,
      "statsInboundDownlink": true
    }
  },
  "api": {
    "tag": "api",
    "services": ["HandlerService", "LoggerService", "StatsService"]
  },
  "stats": {},
  "routing": {
    "domainStrategy": "AsIs",
    "rules": [
      {
        "inboundTag": ["api"],
        "outboundTag": "api",
        "type": "field"
      }
    ],
    "balancers": []
  },
  "inbounds": [
    {
      "listen": "127.0.0.1",
      "port": 8080,
      "protocol": "dokodemo-door",
      "settings": {
        "address": "127.0.0.1"
      },
      "tag": "api"
    },
    {
      "port": 8443,
      "listen": "0.0.0.0",
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "27848739-7e62-4138-9fd3-098a63964b6b",
            "email": "master@v2ray.com",
            "alterId": 16,
            "level": 0
          }
        ],
        "disableInsecureEncryption": true
      },
      "streamSettings": {
        "network": "ws",
        "security": "tls",
        "tlsSettings": {
          "allowInsecure": false,
          "allowInsecureCiphers": false,
          "alpn": ["http/2"],
          "certificates": [
            {
              "usage": "encipherment",
              "certificateFile": "/etc/v2ray/encipherment.crt",
              "keyFile": "/etc/v2ray/encipherment.key"
            }
          ],
          "disableSystemRoot": false
        },
        "wsSettings": {
          "path": "/"
        }
      },
      "tag": "vmess-in-ws-0"
    },
    {
      "port": 57718,
      "listen": "0.0.0.0",
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "27848739-7e62-4138-9fd3-098a63964b6b",
            "email": "master@v2ray.com",
            "alterId": 16,
            "level": 0
          }
        ],
        "disableInsecureEncryption": true
      },
      "streamSettings": {},
      "tag": "vmess-in-0"
    }
  ],
  "outbounds": [
    {
      "sendThrough": "0.0.0.0",
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "AsIs",
        "userLevel": 0
      },
      "tag": "direct",
      "streamSettings": {}
    }
  ],
  "transport": {}
}
